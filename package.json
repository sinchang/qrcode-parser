{
  "name": "qrcode-parser",
  "version": "2.0.0",
  "description": "A pure javascript QR code reading library, accept File object, image url, image base64.",
  "repository": {
    "url": "sinchang/qrcode-parser",
    "type": "git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/qrcodeParser.js",
  "umd:main": "./dist/qrcodeParser.umd.js",
  "module": "./dist/qrcodeParser.es.js",
  "exports": "./dist/qrcodeParser.modern.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "test:cov": "jest --coverage",
    "test": "jest",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "example": "vite",
    "build:example": "vite build",
    "build": "microbundle && npm run build:umd",
    "build:umd": "microbundle -f umd --external none",
    "semantic-release": "semantic-release"
  },
  "author": "sinchang <sinchangwen@gmail.com>",
  "license": "MIT",
  "keywords": [
    "qrcode",
    "base64",
    "parser"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "preset": "ts-jest",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$"
  },
  "dependencies": {
    "jsqr": "^1.4.0",
    "upng-js": "^2.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.8.2",
    "@types/jest": "^27.0.1",
    "@types/mime": "^2.0.3",
    "@types/node": "^16.9.1",
    "@types/upng-js": "^2.1.2",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.4.0",
    "jest": "^27.2.0",
    "jest-environment-jsdom": "^27.2.0",
    "microbundle": "^0.13.3",
    "mime": "^2.5.2",
    "semantic-release": "^17.4.3",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3",
    "vite": "^2.5.6"
  }
}
