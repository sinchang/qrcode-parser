{
  "name": "qrcode-parser",
  "version": "0.0.0-development",
  "description": "A pure javascript QR code reading library, accept File object, image url, image base64.",
  "repository": {
    "url": "sinchang/qrcode-parser",
    "type": "git"
  },
  "main": "dist/qrcode-parser.cjs.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test:cov": "jest --coverage",
    "test": "jest",
    "build": "bili --format cjs,es,umd,umd-min --moduleName qrcodeParser",
    "example": "vite",
    "build:example": "tsc && vite build",
    "semantic-release": "semantic-release"
  },
  "author": "sinchang <sinchangwen@gmail.com>",
  "license": "MIT",
  "jest": {
    "testEnvironment": "jsdom",
    "preset": "ts-jest",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$"
  },
  "dependencies": {
    "jsqr": "^1.4.0",
    "upng-js": "^2.1.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/mime": "^2.0.3",
    "@types/node": "^16.9.1",
    "@types/upng-js": "^2.1.2",
    "jest": "^27.2.0",
    "mime": "^2.5.2",
    "semantic-release": "^17.4.3",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3",
    "vite": "^2.5.6"
  }
